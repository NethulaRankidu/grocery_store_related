	<parameter name="BILL_ID" class="java.lang.Integer"/>
	<query language="SQL"><![CDATA[SELECT
    b.bill_id,
    b.total_price,
    b.datetime,
    c.name AS customer_name,
    c.customer_id AS customer_id,
    a.full_name AS cashier_name,
    p.product_name,
    si.quantity,
    si.unit_price,
    (si.quantity * si.unit_price) AS amount
FROM sold_items si
JOIN bill b ON si.bill_id = b.bill_id
JOIN customer c ON b.customer_id = c.customer_id
JOIN admin a ON b.cashier_id = a.user_id
JOIN products p ON si.product_id = p.product_id
WHERE b.bill_id = $P{BILL_ID}]]></query>
